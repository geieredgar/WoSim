{
  "package": {
    "productName": "wosim-hub",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3001/",
    "beforeDevCommand": "yarn dev -p 3001",
    "beforeBuildCommand": "yarn generate"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "net.wosim.hub",
      "icon": [
        "icons/16x16.png",
        "icons/32x32.png",
        "icons/180x180.png",
        "icons/192x192.png",
        "icons/512x512.png",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "wosim.net",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": true,
      "endpoints": ["https://wosim.net/hub/latest.json"]
    },
    "allowlist": {
      "all": false
    },
    "windows": [
      {
        "title": "WoSim Hub",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
